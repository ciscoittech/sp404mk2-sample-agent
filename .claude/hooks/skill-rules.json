{
  "fastapi-guidelines": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "high",
    "promptTriggers": {
      "keywords": [
        "fastapi", "pydantic", "async", "await", "endpoint", "service",
        "router", "dependency", "middleware", "sqlalchemy", "alembic"
      ],
      "intentPatterns": [
        "(create|add|build).*?(endpoint|api|route)",
        "(implement|integrate).*?(service|model|schema)",
        "(add|create).*?pydantic"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/app/api/**/*.py",
        "backend/app/services/**/*.py",
        "backend/app/models/**/*.py",
        "backend/app/schemas/**/*.py"
      ]
    }
  },
  "audio-processing": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "high",
    "promptTriggers": {
      "keywords": [
        "librosa", "audio", "waveform", "features", "bpm", "key", "tempo",
        "spectral", "mfcc", "chroma", "onset", "harmonic", "percussive",
        "soundfile", "wav", "sample rate"
      ],
      "intentPatterns": [
        "(analyze|extract|detect).*?(audio|bpm|key|tempo)",
        "(process|convert).*?(audio|sample|wav)"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/app/services/audio*.py",
        "backend/app/services/*_features*.py"
      ]
    }
  },
  "hybrid-analysis": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "high",
    "promptTriggers": {
      "keywords": [
        "hybrid analysis", "vibe analysis", "openrouter", "ai analysis",
        "qwen", "model selection", "cost tracking", "api usage",
        "genre classification", "tag generation"
      ],
      "intentPatterns": [
        "(implement|add|enhance).*?(analysis|classification)",
        "(integrate|use).*?(openrouter|ai|model)"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/app/services/hybrid*.py",
        "backend/app/services/openrouter*.py",
        "backend/app/services/*_analysis*.py"
      ]
    }
  },
  "sp404-hardware": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "medium",
    "promptTriggers": {
      "keywords": [
        "sp-404", "sp404", "export", "pad layout", "sample format",
        "hardware", "48khz", "16-bit", "wav", "aiff", "kit builder",
        "pad assignment", "bank"
      ],
      "intentPatterns": [
        "(export|convert).*?(sp-404|sp404|hardware)",
        "(organize|build).*?(kit|pad)",
        "(format|convert).*?(audio|sample)"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/app/services/sp404*.py",
        "backend/app/services/kit*.py",
        "backend/app/api/v1/endpoints/sp404*.py"
      ]
    }
  },
  "database-operations": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "medium",
    "promptTriggers": {
      "keywords": [
        "database", "migration", "alembic", "sqlalchemy", "model",
        "query", "session", "table", "column", "relationship",
        "foreign key", "index", "postgres", "sqlite"
      ],
      "intentPatterns": [
        "(create|modify|update).*?(migration|table|model)",
        "(add|create).*?(column|relationship|index)"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/app/models/**/*.py",
        "backend/alembic/versions/**/*.py",
        "backend/app/db/**/*.py"
      ]
    }
  },
  "testing-patterns": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "medium",
    "promptTriggers": {
      "keywords": [
        "test", "testing", "pytest", "fixture", "mock", "assert",
        "coverage", "e2e", "integration", "unit test", "playwright"
      ],
      "intentPatterns": [
        "(write|create|add).*?test",
        "(test|testing).*?(endpoint|service|model)",
        "(fix|debug).*?test"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/tests/**/*.py",
        "frontend/tests/**/*.js",
        "frontend/tests/**/*.spec.js"
      ]
    }
  },
  "frontend-htmx": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "medium",
    "promptTriggers": {
      "keywords": [
        "htmx", "alpine", "daisyui", "tailwind", "component",
        "template", "jinja2", "form", "modal", "hx-get", "hx-post",
        "hx-swap", "x-data", "x-show"
      ],
      "intentPatterns": [
        "(create|add|build).*?(page|component|template)",
        "(implement|add).*?(htmx|alpine|ui)"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "frontend/pages/**/*.html",
        "frontend/components/**/*.html",
        "backend/templates/**/*.html"
      ]
    }
  },
  "batch-processing": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "low",
    "promptTriggers": {
      "keywords": [
        "batch", "queue", "automation", "cron", "background",
        "async processing", "worker", "parallel", "bulk"
      ],
      "intentPatterns": [
        "(implement|add).*?(batch|queue|automation)",
        "(process|handle).*?(bulk|multiple)"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "backend/app/services/batch*.py",
        "scripts/batch_automation/**/*.py"
      ]
    }
  }
}

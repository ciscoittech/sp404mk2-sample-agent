REVIEW_QUEUE_PATH=./review_queue

# Audio Processing
DEFAULT_SAMPLE_RATE=44100
DEFAULT_BIT_DEPTH=16
MAX_DOWNLOAD_SIZE_MB=100

# Cost Limits
DAILY_TOKEN_LIMIT=100000
COST_ALERT_THRESHOLD_USD=5.00